{
    "name": "amazon-book-ranker",
    "version": "1.0.0",
    "description": "A Node.js application that tracks Amazon book rankings and displays them in a leaderboard format",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "cycle": "node run-cycle.js",
        "volume:check": "node volume-tools.js check",
        "volume:list": "node volume-tools.js list",
        "volume:backup": "node volume-tools.js backup",
        "deploy": "railway up"
    },
    "dependencies": {
        "dotenv": "^16.4.5",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5"
    },
    "devDependencies": {
        "nodemon": "^3.0.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/hxkm/railwayleaderboard.git"
    },
    "author": "",
    "license": "MIT"
} 